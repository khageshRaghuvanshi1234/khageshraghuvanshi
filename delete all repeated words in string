def freq(str):
    str = str.split()         
    str2 = []
    for i in str:       
            str2.append(i) 
              
    for i in range(0, len(str2)):
        print('Frequency of', str2[i], 'is :', str.count(str2[i]))    
  
def main():
    str ='apple mango apple orange orange apple guava mango mango'
    freq(str)                    
  
if _name=="main_":
    main()
